---
layout: none
permalink: /assets/data/team-data.json
---
{
  "team": [
    {%- for member in site.team -%}
    {
      "id": "{{ member.slug }}",
      "name": "{{ member.name | escape }}",
      "role": "{{ member.role | escape }}",
      "bio": "{{ member.bio | strip_html | strip_newlines | escape }}",
      "image": "{{ member.image }}",
      "url": "{{ member.url }}",
      "email": "{{ member.email }}",
      "order": {{ member.order | default: 999 }}
    }{% unless forloop.last %},{% endunless %}
    {%- endfor -%}
  ]
}
